    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sass": "^1.26.10",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "i": "^0.3.6",
    "node-sass": "^4.14.1",
    "npm": "^6.14.8",
    "rollup-plugin-scss": "^2.6.0",
